pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
o={}
smoke_colour={10,9,8,4,5,6}

function _init()
  o.x=63
  o.y=63
  o.smoke={}
end

function make_smoke(p)
  s={}
  -- x pos might be offset
  s.x=p.x
  offset=rnd(9)
  if offset < 4 then s.x-=offset end
  if offset > 6 then s.x+=offset-6 end
  
  -- smoke goes up  
  s.y=p.y
  s.dy=rnd(2)*-1
  
  s.age=1
  s.colour=smoke_colour[1]

  return s
end

function update_smoke(s)
  s.age+=1
  
  if s.age > 60
  then
    del(o.smoke, s)
  else
    if s.age%5==0
    then
      s.colour=smoke_colour[s.age/5+1]
    end
  end
  
  s.y+=s.dy
end

function _update()
  if btn(0) then o.x-=1 end
  if btn(1) then o.x+=1 end
  if btn(2) then o.y-=1 end
  if btn(3) then o.y+=1 end

  foreach(o.smoke, update_smoke)

  if #o.smoke<40 and rnd(10)>1
  then
    add(o.smoke, make_smoke(o))
  end
end

function draw_smoke(s)
  circfill(s.x,s.y,2,s.colour)
end

function _draw()
  cls()
  circfill(o.x,o.y,1,12)
  foreach(o.smoke, draw_smoke)
  print("smoke: "..#o.smoke,3,8,7)
end


