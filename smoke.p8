pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
o={}
smoke_colour={8,10,9,6,5,12}

function _init()
  o.x=63
  o.y=63
  o.smoke={}
end

function make_smoke(p)
  s={}
  -- x pos might be offset
  s.x=p.x
  offset=rnd(9)
  if offset < 4 then s.x-=offset end
  if offset > 6 then s.x+=offset-6 end
  
  -- smoke goes up  
  s.y=p.y
  s.dy=rnd(2)/2*-1
  
  s.age=1
  s.change=2
  s.sprite=16
  s.colour=1

  return s
end

function update_smoke(s)
  s.age+=1
  
  if s.age > 32
  then
    del(o.smoke, s)
  else
    if s.age>s.change
    then
      s.sprite+=1
      s.colour+=1
      s.change*=2
    end
  end
  
  s.y+=s.dy
end

function _update()
  if btn(0) then o.x-=1 end
  if btn(1) then o.x+=1 end
  if btn(2) then o.y-=1 end
  if btn(3) then o.y+=1 end

  foreach(o.smoke, update_smoke)

  if #o.smoke<15 and rnd(5)>3
  then
    add(o.smoke, make_smoke(o))
  end
end

function draw_smoke(s)
  pal(15,smoke_colour[s.colour])
  spr(s.sprite,s.x,s.y)

  -- for comparison
  -- random sprite by age
  ss=s.sprite+flr(rnd(3))*16
  spr(ss,s.x-20,s.y)
  
  -- for comparison
  circfill(s.x-40,s.y,2,smoke_colour[s.colour])
end

function _draw()
  cls()
  circfill(o.x,o.y,1,12)
  for i = #o.smoke,1,-1 do
    draw_smoke(o.smoke[i])
  end
  print("smoke: "..#o.smoke,3,8,7)
end


