pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
aliens={}

-- player ship
-- fixme: move into list!
shipx = 63
shipy = 120
sdx   = 0
sdy   = 0
sa    = 2
ss    = 1

-- bullets
pbullets = {}
bdy   = -3

function moveship()
 sdx=0
 sdy=0

 if btn(0) then sdx = sa*-1 end
 if btn(1) then sdx = sa end
 if btn(2) then sdy = sa*-1 end
 if btn(3) then sdy = sa end
 
 if btnp(4) then firebullet() end
 
 shipx += sdx
 shipy += sdy

 if shipx<0   then shipx=0 end
 if shipx>120 then shipx=120 end
 if shipy<0   then shipy=0 end
 if shipy>120 then shipy=120 end
end

function shipsprite()
 local s=1
 
 if (sdx==0 and sdy!=0) then
  s=2
 elseif (sdx>0) then
  s=3
 elseif (sdx<0) then
  s=4
 end
 
 return s
end 

function drawship()
 spr(shipsprite(),shipx,shipy)
 
 --sspr(8,8,16,16,shipx,shipy)
 
 -- draw half, then again flipped!
 --sspr(8,8,8,16,shipx,shipy)
 --sspr(8,8,8,16,shipx+8,shipy,8,16,1)
end

function firebullet()
 if (#pbullets<5)
 then
  local bullet={["x"]=shipx,["y"]=shipy-1}
  add(pbullets,bullet)
 end
end

function removebullet(b)
 del(pbullets,b)
end 

function movebullet(b)
	b.y += bdy
	if (b.y<0) then removebullet(b) end
end

function drawbullet(b)
 spr(0,b.x,b.y)
end

function new_ship(x,y,n)
  local s={}
  s.x=x
  s.y=y
  s.dx=0
  s.dy=0
  s.a=1
  s.timers={4,20,4}
  s.timer=1
  s.ticks=0
  s.dxtarget={3,0,-3,0}
  s.xmove=0
  s.dytarget={0,1,-1,0,1}
  s.ymove=0
  s.sprite=n
  
  return s
end

function _init()
  add(aliens, new_ship(5,5,8))
end

function move_ship(s)
  if (s.ticks==0) then
    s.ticks=s.timers[s.timer]
    s.timer+=1
    if(s.timer > #s.timers) s.timer=1
    s.xmove+=1
    if(s.xmove > #s.dxtarget) s.xmove=1
    s.ymove+=1
    if(s.ymove > #s.dytarget) s.ymove=1
  end
  
  if(s.dx<s.dxtarget[s.xmove]) then
      s.dx+=s.a
  elseif(s.dx>s.dxtarget[s.xmove]) then
      s.dx-=s.a
  end
  
  if(s.dy<s.dytarget[s.ymove]) then
      s.dy+=s.a
  elseif(s.dy>s.dytarget[s.ymove]) then
      s.dy-=s.a
  end

  s.x+=s.dx  
  s.y+=s.dy
  s.ticks-=1

  if(s.x>127 or s.x<0 or s.y>127 or s.y<0) del(aliens,s)
end

function draw_ship(s)
  --print("ticks:"..s.ticks,2,2)
  --print("dx:"..s.dx,s.x-10,s.y-4)
  --print("dy:"..s.dy,s.x+10,s.y-4)
  spr(s.sprite, s.x, s.y)
end

function _update()
  foreach(aliens, move_ship)
  if(#aliens<3) add(aliens, new_ship(flr(rnd(80)),5,8))

  moveship()
  foreach(pbullets,movebullet)
end

function _draw()
  cls()
  foreach(aliens, draw_ship)

  drawship()
  foreach(pbullets,drawbullet)
end

