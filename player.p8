pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- generic shooter
-- by @johndalton

-- player ship
-- fixme: move into list!
shipx = 63
shipy = 120
sdx   = 0
sdy   = 0
sa    = 2
ss    = 1

-- bullets
pbullets = {}
bdy   = -3

function moveship()
 sdx=0
 sdy=0

 if btn(0) then sdx = sa*-1 end
 if btn(1) then sdx = sa end
 if btn(2) then sdy = sa*-1 end
 if btn(3) then sdy = sa end
 
 if btnp(4) then firebullet() end
 
 shipx += sdx
 shipy += sdy

 if shipx<0   then shipx=0 end
 if shipx>120 then shipx=120 end
 if shipy<0   then shipy=0 end
 if shipy>120 then shipy=120 end
end

function shipsprite()
 local s=1
 
 if (sdx==0 and sdy!=0) then
  s=2
 elseif (sdx>0) then
  s=3
 elseif (sdx<0) then
  s=4
 end
 
 return s
end 

function drawship()
 spr(shipsprite(),shipx,shipy)
 
 --sspr(8,8,16,16,shipx,shipy)
 
 -- draw half, then again flipped!
 --sspr(8,8,8,16,shipx,shipy)
 --sspr(8,8,8,16,shipx+8,shipy,8,16,1)
end

function firebullet()
 if (#pbullets<5)
 then
  local bullet={["x"]=shipx,["y"]=shipy-1}
  add(pbullets,bullet)
 end
end

function removebullet(b)
 del(pbullets,b)
end 

function movebullet(b)
	b.y += bdy
	if (b.y<0) then removebullet(b) end
end

function drawbullet(b)
 spr(0,b.x,b.y)
end

function _update()
 moveship()
 foreach(pbullets,movebullet)
end

function _draw()
 rectfill(0,0, 128,128, 0)
 -- print("x "..shipx,5,5,1)
 -- print("y "..shipy,5,12,1)
 drawship()
 foreach(pbullets,drawbullet)
end
